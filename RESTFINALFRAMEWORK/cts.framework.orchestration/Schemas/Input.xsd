    		</element>
    		<element name="SourceType" type="string" minOccurs="0"
    			maxOccurs="1">
    		</element>
    		<element name="DestinationType" type="string" maxOccurs="1"
    			minOccurs="0">
    		</element>
            <element name="Enrichment" type="string" minOccurs="0" maxOccurs="1"></element>
            <element name="Transformation" type="string" minOccurs="0"
